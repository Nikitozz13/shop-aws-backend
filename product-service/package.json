    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^29.4.0",
    "@types/lambda-tester": "^3.6.2",
    "@types/node": "^14.18.37",
    "aws-sdk": "^2.1328.0",
    "aws-sdk-mock": "^5.8.0",
    "esbuild": "^0.14.11",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "jest": "^29.4.3",
    "json-schema-to-ts": "^1.5.0",
